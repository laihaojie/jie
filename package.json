    "cross-env": "^7.0.3",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.40.0",
    "esno": "^0.16.3",
    "fs-extra": "^10.1.0",
    "rollup": "^2.76.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "typescript": "^5.0.4",
