{
  "name": "@djie/monorepo",
  "version": "1.0.32",
  "private": true,
  "packageManager": "pnpm@8.5.0",
  "author": "laihaojie <https://github.com/laihaojie>",
  "license": "ISC",
  "homepage": "https://github.com/laihaojie/jie#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laihaojie/jie.git"
  },
  "bugs": {
    "url": "https://github.com/laihaojie/jie/issues"
  },
  "scripts": {
    "build:tsup": "esno scripts/tsup.ts",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" rollup -c && pnpm run update",
    "release": "esno scripts/release.ts",
    "publish:ci": "esno scripts/publish.ts",
    "update": "esno scripts/update.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "type:check": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.5.0"
  },
  "devDependencies": {
    "@djie/eslint-config": "^2.1.4",
    "@types/node": "^20.5.8",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "consola": "^3.2.3",
    "cross-env": "^7.0.3",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.56.0",
    "esno": "^0.17.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "rollup": "^3.28.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^6.0.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^5.2.2",
    "unocss": "^0.57.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "esbuild",
        "typescript"
      ]
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md,json}": [
      "eslint --fix"
    ]
  }
}
